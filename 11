fun main() {
    var matriz = arrayOf (
            intArrayOf (1,2,3,4,5),
            intArrayOf (6,7,8,9,10,11),
            intArrayOf (12,13,14,15,16,17,18,19,20)
        )
    for (i in (0 until matriz.size)) {
        println()
        for (j in (0 until matriz[i].size))
            println("Posicion [$i][$j] : ${matriz[i][j]}")
    }
}
