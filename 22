fun main() {
    var array = arrayOf(
    	arrayOf ("Hola",10),
        arrayOf (1,2,3,4,5),
        arrayOf ("Lunes","Martes","Miercoles","Jueves","Viernes")
    )
    for (a in (0 until array.size)) {
        println()
        for (b in (0 until array[a].size)){
            print(" ${array[a][b]} ")
        }
    }
}
